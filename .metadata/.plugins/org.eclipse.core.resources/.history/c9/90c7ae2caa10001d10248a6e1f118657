package arrays;

public class IndexOfArrayElement {
	public static int findIndex(int arr[],int t) {
		if(arr==null) return -1;
		int len=arr.length;
		int i=0;
		while(i<len) {
			if(arr[i]==t) return i;
			else i=i+1;
		}
		return -1;
	}
	public static void main(String[] args) {
		int arr[]= {25,8,45,75,62,13,48};
		System.out.println("index position of 75: "+findIndex(arr, 75));
	}
}
