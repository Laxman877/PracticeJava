package practice;

public class ArrayDemo1 {
	public static void main(String[] args) {
		int a[]= {15,17,19,45,14,13};
		int sum=0;
		int max=a[0];
		int min=a[0];
		for(int i=0;i<a.length;i++) {
			sum+=a[i];
			if(a[i]>max) {
				max=a[i];
				
			}
			if(a[i]<min) {
				min=a[i];
				
			}
		}
		System.out.println(sum);
		System.out.println(max);
		System.out.println(min);
	}
}
